{
  "name": "eggy-tracky",
  "version": "0.1.0",
  "description": "An egg tracker for me and a friend",
  "main": "src/index.js",
  "proxy": "http://localhost:80",
  "scripts": {
    "build": "babel src --out-dir build && cd client && npm install && npm run build",
    "start": "npm run build && node ./build/index.js",
    "start-dev": "nodemon src/index.js --ignore db.json --exec babel-node --presets env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkaden4/eggy-tracky.git"
  },
  "author": "Kaden Thomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkaden4/eggy-tracky/issues"
  },
  "homepage": "https://github.com/tkaden4/eggy-tracky#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0"
  }
}
